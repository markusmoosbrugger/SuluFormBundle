<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>form_data</key>
    <properties>
        <property
                name="id"
                visibility="no"
                translation="sulu_form.id">
            <field-name>id</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="locale"
                visibility="never">
            <field-name>locale</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="firstName"
                visibility="yes"
                translation="sulu_form.type.firstname">
            <field-name>firstName</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="lastName"
                visibility="yes"
                translation="sulu_form.type.lastname">
            <field-name>lastName</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="email"
                visibility="yes"
                translation="sulu_form.email">
            <field-name>email</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="phone"
                visibility="yes"
                translation="sulu_form.type.phone"
                type="number">
            <field-name>phone</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="created"
                translation="sulu_admin.created"
                visibility="yes"
                type="datetime">
            <field-name>created</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
        <property
                name="changed"
                translation="sulu_admin.changed"
                visibility="yes"
                type="datetime">
            <field-name>changed</field-name>
            <entity-name>Sulu\Bundle\FormBundle\Entity\Dynamic</entity-name>
        </property>
    </properties>
</list>